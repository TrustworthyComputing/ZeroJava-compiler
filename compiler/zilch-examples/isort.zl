void main(void) {
	int i;
	int j;
	int arrj;
	int key;
	int N;
	int[] arr;
	arr = new int[4];
	N = 4;
	arr[0] = 4;
	arr[1] = 2;
	arr[2] = 5;
	arr[3] = 1;
	i = 1;
	while (i < N) {
		
		key = arr[i]; 
